dbrp "telegraf"."autogen"

var measurement string
var where_filter = lambda: TRUE
var group_by = [*]
var crit lambda
var warn lambda
var topic string

stream
    |from()
        .measurement(measurement)
        .where(where_filter)
        .groupBy(group_by)
    |alert()
        .warn(warn)
        .crit(crit)
        .stateChangesOnly()
        .topic(topic)