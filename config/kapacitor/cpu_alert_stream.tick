dbrp "telegraf"."autogen"

stream
    |from()
        .measurement('cpu')
        .groupBy('host')
    |alert()
        .crit(lambda: "usage_idle" < 60)
        .warn(lambda: "usage_idle" < 70)
        .stateChangesOnly()
        .topic('cpu')
        .post('http://82.140.0.78:8082/alerts')
            .header('Content-Type', 'application/json')
            .captureResponse()
            .timeout(10s)
